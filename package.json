{
  "name": "roubank",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "parcel index.html",
    "build:parcel": "parcel build index.html --public-url /roubank",
    "build": "npm run clean:windows && npm run build:parcel",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "clean:linux": "rm -rf dist/*",
    "clean:windows": "rd /s/q dist",
    "json:server": "json-server --watch db.json",
    "json:server:routes": "json-server --watch db.json --routes routes.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utfpr-gp/roubank.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/utfpr-gp/roubank/issues"
  },
  "homepage": "https://github.com/utfpr-gp/roubank#readme",
  "dependencies": {
    "alertifyjs": "^1.13.1",
    "jquery": "latest",
    "jquery-mask-plugin": "^1.14.16",
    "materialize-css": "^1.0.0",
    "json-server": "^0.16.3"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "parcel": "^2.4.1"
  }
}
